    <!-- ==================== AK74 ========================== -->
   
   <ThingDef ParentName="BaseHumanGun">
    <defName>Gun_ak74_cwm</defName>
    <label>AK74</label>
    <description>Fielded as the standard Assault Rifle by the Russian Army. The AK-74 is a reliable and rugged medium range weapon.</description>
    <graphicData>
      <texPath>Things/Item/Equipment/Gun_ak74</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <soundInteract>InteractRifle</soundInteract>
    <statBases>
      <MarketValue>600</MarketValue>
      <SightsEfficiency>0.50</SightsEfficiency>
      <ShotSpread>0.07</ShotSpread>
      <SwayFactor>1.40</SwayFactor>
      <Bulk>9.40</Bulk>
      <Mass>3.30</Mass>
      <RangedWeapon_Cooldown>0.4</RangedWeapon_Cooldown>
    </statBases>
    <weaponTags>
      <li>Rifles</li>
    </weaponTags>
	<costList>
	  <Steel>60</Steel>
	  <Component>1</Component>
	  <WoodLog>25</WoodLog>
	</costList>
    <verbs>
      <li Class="CombatExtended.VerbPropertiesCE">
	            <recoilPattern>Regular</recoilPattern>
				<recoilAmount>0.70</recoilAmount>
                <verbClass>CombatExtended.Verb_ShootCE</verbClass>
				<hasStandardCommand>true</hasStandardCommand>
                <projectileDef>Bullet_545x39mmSoviet_FMJ</projectileDef>
				<burstShotCount>6</burstShotCount>
                <ticksBetweenBurstShots>6</ticksBetweenBurstShots>
                <warmupTime>1.1</warmupTime>
                <range>70</range>
                <soundCast>Shotak74</soundCast>
                <soundCastTail>GunTail_Medium</soundCastTail>
                <muzzleFlashScale>5</muzzleFlashScale>
      </li>
    </verbs>
	<comps>
	  <li Class="CombatExtended.CompProperties_AmmoUser">
	    <magazineSize>30</magazineSize>
		<reloadTime>3</reloadTime>
		<ammoSet>AmmoSet_545x39mmSoviet</ammoSet>
	  </li>
	  <li Class="CombatExtended.CompProperties_FireModes">
	    <aimedBurstShotCount>3</aimedBurstShotCount>
		<aiUseBurstMode>TRUE</aiUseBurstMode>
		<aiAimMode>AimedShot</aiAimMode>
	  </li>
	</comps>
